<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Cheeper\DomainModel\Cheep\Cheep" table="cheeps">
        <embedded name="cheepId" class="Cheeper\DomainModel\Cheep\CheepId" />
        <embedded name="authorId" class="Cheeper\DomainModel\Author\AuthorId" />
        <embedded name="cheepMessage" class="Cheeper\DomainModel\Cheep\CheepMessage" />
        <embedded name="cheepDate" class="Cheeper\DomainModel\Cheep\CheepDate" />
    </entity>

</doctrine-mapping>
